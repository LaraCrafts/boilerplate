{
    "name": "laracrafts/boilerplate",
    "description": "A Laravel package boilerplate.",
    "keywords": [
        "laravel",
        "lumen"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.1",
        "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaraCrafts\\Boilerplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraCrafts\\Boilerplate\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Boilerplate": "\\LaraCrafts\\Boilerplate\\Support\\Facades\\Boilerplate"
            },
            "providers": [
                "\\LaraCrafts\\Boilerplate\\BoilerplateServiceProvider"
            ]
        }
    }
}
